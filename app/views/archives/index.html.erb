  <section class="dashboard">
    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        <% @archives.each do |archive| %>
            <div class="col-sm-6 dashboard-menu">
              <div class="row">
                <div class="col-xs-12">
                  <img class="menu-preview" src="<%= archive.preview.url %>" alt="<%= archive.name %>" class="img-responsive">
                </div>
                <div class="col-xs-12 menu-name-wrapper">
                  <p class="menu-name">
                    <%= archive.name %> (Style: <%=  archive.template.name %>)
                  </p>
                  <% if archive.rendered_pdf? %>
                    <a href="<%= archive.rendered_pdf %>" class="pull-right text-default download-menu" download data-toggle="tooltip" title="DOWNLOAD LATEST VERSION OF THIS MENU">
                      <span class="ion ion-ios-cloud-download"></span>
                    </a>
                  <% end %>
                </div>
                <div class="col-xs-12 last-version-wrapper">
                  <p class="last-version"><%= archive.updated_at.strftime("%m/%d/%Y") %></p>
                  <% if archive.rendered_pdf? %>
                    <a href="<%= archive.rendered_pdf %>" target="_blank">
                      <img src="data:image/png;base64," alt="View this menu" data-toggle="tooltip" title="VIEW THIS MENU" class="view-menu">
                    </a>
                  <% end %>
                </div>
              </div>
            </div>
          <div class="col-md-6 hide">
            <img src="<%= archive.preview.url %>" alt="<%= archive.name %>" class="img-responsive">
            <p><%= archive.name %> (<%= archive.template.name %>)</p>
            <p><%= archive.created_at.strftime("%m/%d/%Y") %></p>
            <a href="<%= archive.rendered_pdf %>" target="_blank">View</a>
            <a href="<%= archive.rendered_pdf %>" download>Download</a>
          </div>
        <% end %>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <%= will_paginate @archives, page_links: false %>
      </div>
    </div>
  </section>
